{
  "name": "@sbigtree/egg-plugin-tmp",
  "version": "1.0.0-rc.2",
  "description": "",
  "egg": {
    "typescript": true
  },
  "eggPlugin": {
    "name": "pluginTmp",
    "dependencies": [
      "routeAuth",
      "sequelize"
    ]
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-plugin-tmp",
    "stop": "egg-scripts stop --title=egg-server-plugin-tmp",
    "dev": "egg-bin dev --port 7001",
    "build": "node script/delete-dist.js && tsc -b --clean && tsc && tsc-alias && node dist/SetupPackage.js",
    "publish": "cd dist && npm publish",
    "test-local": "egg-bin test -p",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov -p",
    "ci": "npm run lint && npm run cov && npm run tsc && npm run clean",
    "lint": "eslint . --ext .ts --cache",
    "tsc": "tsc",
    "clean": "tsc -b --clean"
  },
  "dependencies": {
    "@doctormckay/stdlib": "^2.8.1",
    "@eggjs/tegg": "^3.5.2",
    "@eggjs/tegg-aop-plugin": "^3.5.2",
    "@eggjs/tegg-config": "^3.2.3",
    "@eggjs/tegg-controller-plugin": "^3.5.2",
    "@eggjs/tegg-eventbus-plugin": "^3.5.2",
    "@eggjs/tegg-plugin": "^3.5.2",
    "@eggjs/tegg-schedule-plugin": "^3.5.2",
    "@elastic/elasticsearch": "^8.10.0",
    "@sbigtree/db-model": "^1.1.6-rc11",
    "@sbigtree/egg-plugin-sequelize": "^1.1.0-rc.0",
    "@sbigtree/egg-route-auth": "^1.1.5-rc0",
    "egg": "^3.15.0",
    "egg-mysql": "^4.0.0",
    "egg-scripts": "^2.17.0",
    "egg-tracer": "^2.0.0",
    "egg-validate": "^2.0.2",
    "itsdangerjs": "^0.0.1",
    "log4js": "^6.9.1",
    "nacos": "^2.6.0",
    "redis": "^4.6.9",
    "sequelize": "^6.33.0",
    "tracer": "^1.3.0",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "1",
    "@types/mocha": "10",
    "@types/node": "18",
    "egg-bin": "6",
    "egg-mock": "5",
    "eslint": "8",
    "eslint-config-egg": "12",
    "typescript": "4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "overrides": {
    "undici": "^5.22.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": " https://github.com/sbigtree/egg-plugin-tmp.git"
  },
  "author": "",
  "license": "MIT"
}
